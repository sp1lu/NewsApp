<% layout('layouts/boilerplate') %>

<div class="title-section title-section-left">
    <h1>Add new feed</h1>
</div>
<div class="user-form">
    <form action="/preferences/add" method="POST">
        <div class="add-custom-input add-custom-name">
            <label for="name">Name</label>
            <input type="text" id="name" name="channel[name]" placeholder="Name" required>
        </div>
        <div class="add-custom-input add-custom-url">
            <label for="url">Url</label>
            <input type="text" id="url" name="channel[url]" placeholder="Url" required>            
        </div>
        <button type="submit" class="btn-primary">Add</button>
    </form>
</div>