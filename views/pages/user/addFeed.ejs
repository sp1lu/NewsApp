<% layout('layouts/boilerplate') %>

<div>
    <h1>Add new feed</h1>
</div>
<div>
    <form action="/preferences/add" method="POST">
        <div>
            <label for="name">Name</label>
            <input type="text" id="name" name="channel[name]" placeholder="Name" required>
        </div>
        <div>
            <label for="url">Url</label>
            <input type="text" id="url" name="channel[url]" placeholder="Url" required>            
        </div>
        <button type="submit">Add</button>
    </form>
</div>